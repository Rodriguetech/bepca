{% extends 'base.html.twig' %}

{% block title %}Administration{% endblock %}

{% block body %}
    <br><br>

    <div class="pt-120 pb-130" id="administration">
        <div class="container">
            <div class="row mb-90">
                <div class="col-lg-6 offset-lg-3 text-center"><span class="badge bg-light text-dark mb-20" data-show="startbox">Administration</span>
                    <h2 class="m-0 px-lg-70" data-show="startbox" data-show-delay="100">
                        Chairman and council members of the board of directors
                    </h2>
                </div>
            </div><!-- Slider-->
            <div class="swiper mt-90" data-swiper-slides="1" data-swiper-breakpoints="828:2, 1024:4" data-swiper-loop="true" data-swiper-gap="30" data-swiper-grabcursor="true" data-show="startbox">
                <div class="swiper-container">
                    <div class="swiper-wrapper">
                        <div class="swiper-slide">
                            <!-- Industry-->
                            <div class="industry position-relative rounded-4 overflow-hidden"><a class="industry-link stretched-link" href="#"></a>
                                <div class="industry-image" data-img-height style="--img-height: 114%;">
                                    <img loading="lazy" src="assets/photo1.jpg" alt=""></div>
                                <div class="industry-body">
                                    <div class="industry-icon me-20 text-white"></div>
                                    <h4 class="industry-title m-0 text-white">
                                        Chairman
                                    </h4>
                                </div>
                            </div>
                        </div>
                        <div class="swiper-slide">
                            <!-- Industry-->
                            <div class="industry position-relative rounded-4 overflow-hidden"><a class="industry-link stretched-link" href="#"></a>
                                <div class="industry-image" data-img-height style="--img-height: 114%;"><img loading="lazy" src="assets/photo2.jpg" alt=""></div>
                                <div class="industry-body">
                                    <div class="industry-icon me-20 text-white"></div>
                                    <h4 class="industry-title m-0 text-white">
                                        Council Member
                                    </h4>
                                </div>
                            </div>
                        </div>
                        <div class="swiper-slide">
                            <!-- Industry-->
                            <div class="industry position-relative rounded-4 overflow-hidden"><a class="industry-link stretched-link" href="#"></a>
                                <div class="industry-image" data-img-height style="--img-height: 114%;"><img loading="lazy" src="assets/photo3.jpg" alt=""></div>
                                <div class="industry-body">
                                    <div class="industry-icon me-20 text-white"></div>
                                    <h4 class="industry-title m-0 text-white">
                                        Council Member
                                    </h4>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="d-flex mt-70 justify-content-center">
                    <div class="swiper-button-prev swiper-button-position-2 swiper-button-gray shadow"><svg xmlns="http://www.w3.org/2000/svg" width="20" height="14" fill="none">
                            <path fill="currentColor" fill-rule="evenodd" d="m3.96 6.15 5.08-4.515L7.91.365.445 7l7.465 6.635 1.13-1.27L3.96 7.85h15.765v-1.7H3.96Z" clip-rule="evenodd" />
                        </svg></div>
                    <div class="swiper-button-next swiper-button-position-2 swiper-button-gray shadow"><svg xmlns="http://www.w3.org/2000/svg" width="20" height="14" fill="none">
                            <path fill="currentColor" fill-rule="evenodd" d="m16.21 6.15-5.08-4.515 1.13-1.27L19.725 7l-7.465 6.635-1.13-1.27 5.08-4.515H.445v-1.7H16.21Z" clip-rule="evenodd" />
                        </svg></div>
                </div>
            </div>
        </div>
    </div>

{% endblock %}
